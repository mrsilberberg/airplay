{
  "name": "Shairport External",
  "version": "1.0.0",
  "slug": "shairport_external",
  "description": "Add-on to run the shairport-sync external Docker image using host network and ALSA passthrough.",
  "startup": "services",
  "boot": "auto",
  "homeassistant": "2023.0.0",
  "arch": [
    "amd64",
    "armv7",
    "aarch64"
  ],
  "options": {},
  "schema": {},
  "init": false,
  "hassio_api": false,
  "hassio_role": "default",
  "homeassistant_api": false,
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "host_network": true,
  "full_access": true,
  "usb": true,
  "map": [
    "config:rw",
    "addons:rw",
    "ssl:rw",
    "backup:rw",
    "share:rw",
    "media:rw",
    "data:rw"
  ],
  "image": "mikebrady/shairport-sync"
}
